{
  "name": "iamdual/srt-parser",
  "description": "A SRT (SubRip text) parser library.",
  "homepage": "https://github.com/iamdual/srt-parser",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Ekin Karadeniz",
      "email": "iamdual@icloud.com"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Iamdual\\SrtParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iamdual\\SrtParser\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "php ./vendor/bin/phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}